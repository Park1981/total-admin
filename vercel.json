{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node@3.0.0",
      "config": {
        "nodeVersion": "18.x"
      }
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}